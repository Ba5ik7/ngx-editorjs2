{
  "name": "ngx-editorjs2-demo",
  "version": "19.0.0",
  "scripts": {
    "build-demo": "ng build ngx-editorjs2-demo --configuration production --base-href \"/ngx-editorjs2/\"",
    "build-ngx-editor-js2": "ng build ngx-editor-js2 --configuration production && npm run postbuild",
    "watch-demo": "ng serve ngx-editorjs2-demo",
    "ngx-editor-js2:watch": "ng build ngx-editor-js2 --watch --configuration development",
    "ngx-editor-js2:postbuild": "chokidar 'dist/ngx-editor-js2/**/*' -i 'dist/ngx-editor-js2/styles/**/*' -c 'npm run postbuild'",
    "ngx-editor-js2:watch:postbuild": "npm-run-all --parallel ngx-editor-js2:watch ngx-editor-js2:postbuild",
    "test": "ng test ngx-editor-js2 --coverage",
    "test-jest": "jest",
    "deployDemo": "npx angular-cli-ghpages --dir=dist/ngx-editorjs2-demo/browser",
    "postbuild": "node ./postbuild.js"
  },
  "private": false,
  "dependencies": {
    "@angular/animations": "^19.0.0",
    "@angular/cdk": "^19.1.0",
    "@angular/common": "^19.0.0",
    "@angular/compiler": "^19.0.0",
    "@angular/core": "^19.0.0",
    "@angular/forms": "^19.0.0",
    "@angular/material": "^19.1.0",
    "@angular/platform-browser": "^19.0.0",
    "@angular/platform-browser-dynamic": "^19.0.0",
    "@angular/router": "^19.0.0",
    "material": "^0.9.21",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "^0.15.0"
  },
  "devDependencies": {
    "@angular-builders/jest": "^19.0.0",
    "@angular-devkit/build-angular": "^19.1.2",
    "@angular/build": "^19.1.2",
    "@angular/cli": "^19.0.6",
    "@angular/compiler-cli": "^19.0.0",
    "@types/jest": "^29.5.14",
    "chokidar-cli": "^3.0.0",
    "jest": "^29.7.0",
    "jest-preset-angular": "^14.5.1",
    "ng-mocks": "^14.13.2",
    "ng-packagr": "^19.1.0",
    "npm-run-all": "^4.1.5",
    "ts-jest": "^29.2.5",
    "typescript": "~5.6.2"
  }
}
