{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "ngx-editorjs2-demo": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        }
      },
      "root": "projects/ngx-editorjs2-demo",
      "sourceRoot": "projects/ngx-editorjs2-demo/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:application",
          "options": {
            "outputPath": "dist/ngx-editorjs2-demo",
            "index": "projects/ngx-editorjs2-demo/src/index.html",
            "browser": "projects/ngx-editorjs2-demo/src/main.ts",
            "polyfills": [
              "zone.js"
            ],
            "tsConfig": "projects/ngx-editorjs2-demo/tsconfig.app.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              {
                "glob": "**/*",
                "input": "projects/ngx-editorjs2-demo/public"
              }
            ],
            "styles": [
              "@angular/material/prebuilt-themes/azure-blue.css",
              "projects/ngx-editorjs2-demo/src/styles.scss"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kB",
                  "maximumError": "1MB"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "4kB",
                  "maximumError": "8kB"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "optimization": false,
              "extractLicenses": false,
              "sourceMap": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "buildTarget": "ngx-editorjs2-demo:build:production"
            },
            "development": {
              "buildTarget": "ngx-editorjs2-demo:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n"
        },
        "test": {
          "builder": "@angular-builders/jest:run",
          "options": {
            "polyfills": [
              "zone.js",
              "zone.js/testing"
            ],
            "tsConfig": "tsconfig.spec.json",
            "inlineStyleLanguage": ["scss"],
            "styles": [
              "@angular/material/prebuilt-themes/azure-blue.css",
              "projects/ngx-editorjs2-demo/src/styles.scss"
            ],
            "scripts": []
          }
        }
      }
    },
    "ngx-editor-js2": {
      "projectType": "library",
      "root": "projects/ngx-editor-js2",
      "sourceRoot": "projects/ngx-editor-js2/src",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular/build:ng-packagr",
          "options": {
            "project": "projects/ngx-editor-js2/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "projects/ngx-editor-js2/tsconfig.lib.prod.json"
            },
            "development": {
              "tsConfig": "projects/ngx-editor-js2/tsconfig.lib.json"
            }
          },
          "defaultConfiguration": "production"
        },
        "test": {
          "builder": "@angular-builders/jest:run",
          "options": {
            "tsConfig": "tsconfig.spec.json",
            "polyfills": [
              "zone.js",
              "zone.js/testing"
            ]
          }
        }
      }
    },
    "ngx-editor-js2-image": {
      "projectType": "library",
      "root": "projects/ngx-editor-js2-image",
      "sourceRoot": "projects/ngx-editor-js2-image/src",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular/build:ng-packagr",
          "options": {
            "project": "projects/ngx-editor-js2-image/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "projects/ngx-editor-js2-image/tsconfig.lib.prod.json"
            },
            "development": {
              "tsConfig": "projects/ngx-editor-js2-image/tsconfig.lib.json"
            }
          },
          "defaultConfiguration": "production"
        },
        "test": {
          "builder": "@angular-builders/jest:run",
          "options": {
            "tsConfig": "tsconfig.spec.json",
            "polyfills": [
              "zone.js",
              "zone.js/testing"
            ]
          }
        }
      }
    }
  },
  "cli": {
    "analytics": "deed997f-88f1-48ad-bbce-a94450742ba5"
  }
}
